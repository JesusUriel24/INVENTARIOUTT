<main class="contenedor contenedor-formularios">
    <h1><%= nombrePagina %> </h1>
    <form action="/crear_cuenta" method="post" class="default-form">
        <div class="campo">
            <label>Nombre</label>
            <input type="text" name="nombre" placeholder="Nombre completo del docente">
        </div>

        <div class="campo">
            <label>Email</label>
            <input type="email" name="email" placeholder="Correo ElectrÃ³nico">
        </div>

        <div class="campo">
            <label>Password</label>
            <input type="password" name="password" placeholder="Password">
        </div>

        <div class="campo">
            <label>Repetir Password</label>
            <input type="password" name="confirmar" placeholder="Ingresa nuevamente tu password">
        </div>
        <div class="campo enviar">
            <input type="submit" class="btn btn-verde" value="Crear Cuenta">
        </div>
    </form>
</main>